<p id="notice"><%= notice %></p>

<h1>档案列表</h1>

<table>
  <thead>
    <tr>
      <th>档案号</th>
      <th>姓名</th>
      <th>机要号</th>
      <th>是否调档</th>
      <th>调档方式</th>
      <th>是否读研</th>
      <th>第一次调档</th>
      <th>第二次调档</th>
      <th>高中材料</th>
      <th>本科材料</th>
      <th>硕士材料</th>
      <th>博士材料</th>
      <th>党员材料</th>
      <th>其他材料</th>
      <th>专升本材料</th>
      <th>备注</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @archives.each do |archive| %>
      <tr>
        <td><%= archive.archiveno %></td>
        <td><%= archive.student.name %></td>
        <td><%= archive.jiyaono %></td>
        <td><%= archive.issend %></td>
        <td><%= archive.sendway %></td>
        <td><%= archive.isduyan %></td>
        <td><%= archive.firstsend %></td>
        <td><%= archive.secondsend %></td>
        <td><%= archive.hmaterial %></td>
        <td><%= archive.umaterial %></td>
        <td><%= archive.mmaterial %></td>
        <td><%= archive.dmaterial %></td>
        <td><%= archive.pmaterial %></td>
        <td><%= archive.omaterial %></td>
        <td><%= archive.zmaterial %></td>
        <td><%= archive.remark %></td>
        <td><%= link_to 'Show', archive %></td>
        <td><%= link_to 'Edit', edit_archive_path(archive) %></td>
        <td><%= link_to 'Destroy', archive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

